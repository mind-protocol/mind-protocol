# L2 View Resolver Configuration (Local Dev)
# Point to local FalkorDB (once data is imported locally)

# Environment
ENV=development

# FalkorDB Configuration (Local)
FALKORDB_MODE=local
FALKORDB_HOST=localhost
FALKORDB_PORT=6379
GRAPH_NAME=scopelock

# Membrane Bus Configuration
MEMBRANE_OBSERVE_URI=ws://localhost:8765/observe
MEMBRANE_INJECT_URI=ws://localhost:8765/inject

# Org Configuration
MEMBRANE_ORG=scopelock

# CPS-1 Economy (Soft-enforce for dev)
CPS1_ENFORCE=false

# Cache Configuration
CACHE_TTL_SECONDS=300
